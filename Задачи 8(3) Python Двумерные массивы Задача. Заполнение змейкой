#Hapilin
n,m = [int(i) for i in input().split()]
zmeika = [["*"]*m for i in range(n)]
z = int(1)
for i in range(n):
    if i%2 == 0:
        for j in range(m):
            zmeika[i][j]=z
            z+=1
    else:
        for j in range(-1,(-m)-1,-1):
            zmeika[i][j]=z
            z+=1
for row in zmeika:
    print(*row)
  

#Enotov
n, m = map(int, input().split())
for j in range(n):
    print(' '.join(['{:>4}'.format(i + 1 + m * j) for i in range(m)][::pow(-1, j)]))


#Netyaga
a, b = [int(i) for i in input().split()]
k = [str(i) for i in range(1, (int(a*b) + 1))]
cc = []
h = []
for i in range(a):
    h = k[0:b]
    for j in range(b):
        k.pop(0)
    if (i%2) == 0:
        cc.append(h)
    else:
        cc.append(list(reversed(h)))
for i in cc:
    print(*i)
        
        
#Lysenko
n, m = map(int, input().split())
k=[]
for i in range(m*n):
    k.append(i+1)
for h in range(n):
    if h%2==0:
        print(*(k[m*h:m+m*h]))
    else:
        print(*list(reversed(k[m*h:m+m*h])))
        

