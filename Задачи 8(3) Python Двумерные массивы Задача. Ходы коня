#Hapilin
y,x=input().split()
chisla={'a':0,'b':1,'c':2,'d':3,'e':4,'f':5,'g':6,'h':7}
y = chisla[y]
x = int(x) - 1 
doska=[["."]*8 for i in range(8)]
doska[x][y] = 'K'
for i in range(8):
    for j in range(8):
        if abs(i - y)*abs(j - x) == 2:
            doska[j][i]='*'
for row in doska:
    print(*row)
    
    
#Netyaga
x, y = list(input())
x = int(ord(x) - 95)
y = 8 - int(y)
ta = [['.' for i in range(10)] for j in range(10)]
ta[y][x] = 'K'
ta[y+2][x-1] = '*'
ta[y+2][x+1] = '*'
ta[y-2][x-1] = '*'
ta[y-2][x+1] = '*'
ta[y+1][x-2] = '*'
ta[y+1][x+2] = '*'
ta[y-1][x-2] = '*'
ta[y-1][x+2] = '*'
for i in ta[0:-2]:
    print(*i[2:])
