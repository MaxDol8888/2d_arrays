def transpose(sp):
    sp1 = []
    for k in range(n):
        sp1.append([])
    r = int(0)
    c = int(0)
    while r < n:
        while c < n:
            sp1[r].append(sp[c][r])
            c += 1
        c = int(0)
        r+=1
    for strok in sp1:
        print(*strok)    
n = int(input())
sp = [[i for i in input().split()] for j in range(n)]
transpose(sp)

