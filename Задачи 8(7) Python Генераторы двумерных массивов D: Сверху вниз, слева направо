#Hapilin
n = int(input())
m = int(input())
a = int(0)
sp = [[i + n*j for j in range(m)] for i in range(n)]
for row in sp:
    print(*row)


#Redkach
m=int(input())
n=int(input())
print([[ j*5+i for j in range(m)] for i in range(n)])


#Lysenko
n=int(input())
m=int(input())
ki=[ [j*n+i for j in range(m)] for i in range(n)]
for i in range(len(ki)):
     print(*ki[i])
     
     
