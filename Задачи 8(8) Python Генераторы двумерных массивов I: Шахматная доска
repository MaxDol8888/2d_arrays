#Hapilin
n = int(input())
m = int(input())
z = ["1" if j%2!=1 else "0" for j in range(m)]
m = ["0" if j%2==0 else "1" for j in range(m)]
sp = [z if i%2==0 else m for i in range(n)]
for row in sp:
    print(*row)


#Alina
a=[] 
m=int(input()) 
n=int(input()) 
a=[[0]*m for i in range (n)] 
for i in range(n): 
    for j in range(m):
        if (i+j)%2:
            a[i][j] = 0
        else:
            a[i][j] = 1
for row in a : 
    print(*row)
    
    
